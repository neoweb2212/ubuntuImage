server {
    listen 80;
    server_name zxz.fr www.zxz.fr;

    location /emailfinder {
        include uwsgi_params;
        uwsgi_pass unix:/run/uwsgi/emailfinder.sock;
    }

    location /websitecrawl {
        include uwsgi_params;
        uwsgi_pass unix:/run/uwsgi/websitecrawl.sock;
    }

    # Ajoutez d'autres locations pour les futurs projets ici
}
